{
  "name": "@mazuhim/api-favorites",
  "version": "1.0.0",
  "description": "Micro serviço de transações resgate/acumulo.",
  "main": "src/index.js",
  "scripts": {
    "migration:up": "./node_modules/.bin/knex migrate:latest",
    "start": "npm run migration:up && pm2 start process.yml",
    "dev": "nodemon src/index.js",
    "test": "npm run lint && npm run test:unit && npm run test:integration",
    "jest": "NODE_ENV=test node_modules/.bin/jest --watch",
    "test:unit": "mocha --opts test/unit/mocha.opts 'test/unit/**/*.js'",
    "test:integration": "mocha --opts test/integration/mocha.opts 'test/integration/**/*.js'",
    "lint": "eslint src test",
    "cover": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Mazuhim/api-favorites"
  },
  "author": "Thiago Mazuhim",
  "license": "GNU",
