    "@hapi/joi": "^15.0.3",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "cron": "^1.5.1",
    "crypto-js": "^3.1.9-1",
    "debug": "^4.1.0",
    "dotenv": "^6.1.0",
    "elastic-apm-node": "^2.10.0",
    "express": "^4.16.4",
    "express-swagger-generator": "^1.1.17",
    "express-swaggerize-ui": "^1.1.0",
    "helmet": "^3.15.0",
    "http-errors": "^1.7.2",
